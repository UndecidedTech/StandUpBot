generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model StandUps {
  id   String   @id @default(auto()) @map("_id") @db.ObjectId
  date DateTime
}

model Users {
  id           String @id @default(auto()) @map("_id") @db.ObjectId
  username     String
  discordId    String
  avatar       String
  refreshToken String
  accessToken  String
}
